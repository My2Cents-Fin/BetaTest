{
  "crons": [
    {
      "path": "/api/cron/send-notifications",
      "schedule": "30 3 * * *"
    },
    {
      "path": "/api/cron/send-notifications",
      "schedule": "30 14 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
